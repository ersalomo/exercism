{
  "compilerOptions": {
    "target": "ESNext",
    "experimentalDecorators": true,
    "module": "commonjs",
    "baseUrl": "./",
    "paths": {
      "*" :["node_modules/*"]
    },
    "typeRoots": ["./node_modules/@types"],
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
